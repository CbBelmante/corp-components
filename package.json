{
  "name": "corp-components",
  "version": "0.1.0",
  "description": "Vue 3 Component Library - TypeScript First",
  "type": "module",
  "main": "./dist/corp-components.umd.cjs",
  "module": "./dist/corp-components.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/corp-components.js",
      "require": "./dist/corp-components.umd.cjs"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "concurrently \"vite\" \"vitepress dev docs/public --port 2224\"",
    "devDocs": "vitepress dev docs/public --port 2224",
    "devPlayground": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "buildDocs": "vitepress build docs/public",
    "preview": "vitepress preview docs/public",
    "typecheck": "vue-tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "peerDependencies": {
    "vue": "^3.4.0"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-vue-next": "^0.544.0",
    "reka-ui": "^2.7.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "^22.16.5",
    "@vitejs/plugin-vue": "^6.0.1",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "~5.8.0",
    "vite": "^7.0.6",
    "vitepress": "^1.6.4",
    "vue": "^3.5.18",
    "vue-router": "^4.6.4",
    "vue-tsc": "^3.0.4"
  },
  "keywords": [
    "vue",
    "vue3",
    "components",
    "typescript",
    "ui",
    "forms",
    "shadcn",
    "shadcn-vue",
    "reka-ui",
    "tailwindcss"
  ],
  "author": "CbBelmante",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CbBelmante/corp-components.git"
  },
  "bugs": {
    "url": "https://github.com/CbBelmante/corp-components/issues"
  },
  "homepage": "https://github.com/CbBelmante/corp-components#readme"
}
