{
  "name": "corp-components",
  "version": "0.1.0",
  "description": "Vue 3 Component Library - TypeScript First",
  "type": "module",
  "main": "./dist/corp-components.umd.cjs",
  "module": "./dist/corp-components.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/corp-components.js",
      "require": "./dist/corp-components.umd.cjs"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsx scripts/dev.ts",
    "devDocs": "tsx scripts/devDocs.ts",
    "devPlayground": "tsx scripts/devPlayground.ts",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "buildDocs": "vitepress build docs/public",
    "preview": "vitepress preview docs/public",
    "typecheck": "vue-tsc --noEmit",
    "tscw": "vue-tsc --noEmit --watch",
    "syncAliases": "tsx scripts/syncAliases.ts",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "lintFix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "prepublishOnly": "npm run build",
    "prepare": "husky"
  },
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "peerDependencies": {
    "vue": "^3.4.0",
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-brands-svg-icons": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/vue-fontawesome": "^3.1.2"
  },
  "peerDependenciesMeta": {
    "@fortawesome/fontawesome-svg-core": {
      "optional": true
    },
    "@fortawesome/free-brands-svg-icons": {
      "optional": true
    },
    "@fortawesome/free-solid-svg-icons": {
      "optional": true
    },
    "@fortawesome/vue-fontawesome": {
      "optional": true
    }
  },
  "dependencies": {
    "@vueuse/core": "^14.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-vue-next": "^0.544.0",
    "reka-ui": "^2.7.0",
    "tailwind-merge": "^3.4.0",
    "vue-i18n": "^11.2.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "^22.16.5",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "eslint": "^9.36.0",
    "eslint-plugin-vue": "^10.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.19",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.21.0",
    "typescript": "~5.8.0",
    "vite": "^7.0.6",
    "vitepress": "^1.6.4",
    "vue": "^3.5.18",
    "vue-router": "^4.6.4",
    "vue-tsc": "^3.0.4"
  },
  "keywords": [
    "vue",
    "vue3",
    "components",
    "typescript",
    "ui",
    "forms",
    "shadcn",
    "shadcn-vue",
    "reka-ui",
    "tailwindcss"
  ],
  "author": "CbBelmante",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CbBelmante/corp-components.git"
  },
  "bugs": {
    "url": "https://github.com/CbBelmante/corp-components/issues"
  },
  "homepage": "https://github.com/CbBelmante/corp-components#readme",
  "lint-staged": {
    "*.{vue,js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,html,css,scss}": [
      "prettier --write"
    ]
  }
}
